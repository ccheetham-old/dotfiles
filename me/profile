# -*- sh -*-

case $OSTYPE in
    linux*)
        MY_OS=linux
        ;;
    solaris*)
        MY_OS=solaris
        ;;
esac

export MY_ARCH=~/local/${MY_OS}-$(uname -m)
export MY_ARCH_OPT=$MY_ARCH/opt
export MY_NOARCH=~/local
export MY_NOARCH_OPT=$MY_NOARCH/opt

source ~/.me/java
source ~/.me/paths
source ~/.me/aliases
