label.0=dup
path.0=mbox:/home/cheetos/var/mail/duplicates
notify.0=beep
action.0=xmutt -f /home/cheetos/var/mail/duplicates

label.1=def
path.1=mbox:/home/cheetos/var/mail/default
notify.1=beep
action.1=xmutt -f /home/cheetos/var/mail/default

label.2=nov
path.2=mbox:/home/cheetos/var/mail/novell
notify.2=beep
action.2=xmutt -f /home/cheetos/var/mail/novell
