[begin] (cheetos)
[encoding] {UTF-8}
  [exec] (browser) {chromium}
  [exec]   (term) {xterm}
  [exec]   (root) {xterm -T "root" -e sudo su -}
  [submenu] (places)
    [exec]   (cnc windows) {rdesktop -u ccheetham -d s2qa -p 'reds0x08;;' -g 1280x800 -T "vmc-ssrc-2k890 (CNC Windows)" vmc-ssrc-2k890}
  [end]
  [submenu] (apps)
    [exec]   (alsamixer) {xterm -e alsamixer}
    [exec]   (alsaplayer) {alsaplayer}
    [exec]   (emacs) {emacs ~/workspace}
    [exec]   (eog) {eog}
    [exec]   (evince) {evince}
    [exec]   (firefox) {firefox}
    [exec]   (gimp) {gimp}
    [exec]   (pidgin) {pidgin}
    [exec]   (mutt) {xmutt}
    [exec]   (netbeans) {netbeans}
    [exec]   (vncviewer) {vncviewer}
    [exec]   (vpn) {vpn}
    [exec]   (wireshark) {wireshark}
    [separator]
    [exec]   (view doc...) {FILE=$(Xdialog --stdout --title "Select pdf, ps, tiff, or dvi" --fselect ~ 0 0) && evince-previewer $FILE}
    [exec]   (view manpage...) {MANPAGE=$(Xdialog --stdout --title "Man Page" --inputbox "Enter man page" 0 0) && yelp man:$MANPAGE}
  [end]
  [submenu] (dev)
    [exec]   (connector) {xterm -T 'Apache Connector' -name ncss -e 'cd $APACHE_CONNECTOR && make run'}
    [separator]
    [exec]   (connector client) {xterm -T 'Connector Client' -name ncss -e 'console -u http://localhost:9980/ -e'}
    [exec]   (event server) {xterm -T 'Event Server' -name ncss -e eventserver}
    [separator]
    [exec]   (mfc cloud controller log) {xterm -T 'MCF Cloud Foundry Log' -name log $MFC_CC_LOGCMD}
    [exec]   (event log) {xterm -T 'Event Log' -name log -e smarttail $NCSS_HOME/var/log/connector/event.log}
    [exec]   (twistd log) {xterm -T 'Twisted Log' -name log -e smarttail $NCSS_HOME/var/log/connector/twistd.log}
    [exec]   (twistd err) {xterm -T 'Twisted Errors' -name log -e smarttail $NCSS_HOME/var/log/connector/twistd.err}
  [end]
  [submenu] (office)
    [exec]  (draw) {oodraw}
    [exec]  (presentation) {ooimpress}
    [exec]  (spreadhseet) {oocalc}
    [exec]  (word processor) {oowriter}
    [separator]
    [exec]  (document) {soffice}
    [separator]
    [exec]  (database) {oobase}
    [exec]  (math) {oomath}
    [exec]  (print) {ooffice-printeradmin}
  [end]
  [submenu] (logs)
    [exec]   (apache access) {xterm -name log -T "Apache Access Log" -e tail -f /var/log/apache2/access_log}
    [exec]   (apache error) {xterm -name log -T "Apache Error Log" -e tail -f /var/log/apache2/error_log}
    [exec]   (rabbitmq) {xterm -name log -T "RabbitMQ Log" -e tail -f /var/log/rabbitmq/rabbit@$(uname -n).log}
    [exec]   (netbeans) {xterm -name log -T "NetBeans Log" -e tail -f ~/.netbeans/6.9/var/log/messages.log}
    [exec]   (system) {xterm -name log -T "System Log" -e sudo tail -f /var/log/messages}
  [end]
  [separator]
  [submenu] (desktop)
    [exec] (screenshot) {import screenshot.png && display -title screenshot.png -resize 50% screenshot.png}
    [exec] (xfontsel) {xfontsel}
    [exec] (load .Xdefaults) {xrdb -merge $HOME/.Xdefaults}
    [exec] (window name) {xprop WM_CLASS | cut -d \" -f 2|xmessage -file - -center}
#    [separator]
#    [exec] (lock) {xscreensaver-command -lock}
    [submenu] (fluxbox)
      [config] (configure)
      [submenu] (styles)
        [include] (/usr/share/fluxbox/menu.d/styles/)
      [end]
      [submenu] (tools)
      [end]
      [commanddialog] (Fluxbox Command)
      [reconfig] (reload config)
      [restart] (restart)
      [exec] (about) {(fluxbox -v; fluxbox -info | sed 1d) | xmessage -file - -center}
  [end]
  [end]
  [submenu] (system)
    [exec]   (top) {xterm -T "Top" -e top}
    [separator]
    [exec]   (reboot) {sudo reboot}
    [exec]   (poweroff) {sudo poweroff}
    [exec]   (single user) {sudo init 1}
  [end]
  [workspaces] (workspace list)
  [separator]
  [exit] (exit)
[endencoding]
[end]
